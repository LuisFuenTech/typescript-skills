  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Arrays",
      "program": "${file}",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "outFiles": ["${workspaceFolder}/**/*.js"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Fundamentals",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/typescript-fundamentals/dist/app.js",
      "outFiles": ["${workspaceFolder}/**/*.js"],
      "preLaunchTask": "compiling_typeScript",
      "internalConsoleOptions": "openOnSessionStart"
    }
  ]
}
